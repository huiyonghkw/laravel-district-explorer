{
    "name": "bravist/laravel-district-explorer",
    "description": "District Explorer For Laravel with AutoNavi. 中国行政区划数据，访问高德在线数据获取最新区划数据。",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^6.0|^6.3",
        "overtrue/laravel-pinyin": "~4.0",
        "illuminate/support": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "会勇禾口王",
            "email": "huiyonghkw@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Bravist\\DistrictExplorer\\ServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Bravist\\DistrictExplorer\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
